package com.kodewala;

import org.springframework.context.support.ClassPathXmlApplicationContext;

public class App 
{
    	public static void main(String[] args) {
    	       
    		String config = "com\\kodewala\\resources\\applicationContext.XML";
    		
    		ClassPathXmlApplicationContext applicationContext = new ClassPathXmlApplicationContext(config);

    		School scc = (School) applicationContext.getBean("acc");
    		System.out.println(scc.getTeachersName() + " " + scc.getTeachersId() + " " + scc.getStudentsId() + " " + scc.getStudentsId());
    	}
}
